#!/bin/bash


#check docker daemon is available
nc -vz   timeout 60 unix//...

/var/vcap/packages/docker/bin/docker load /var/vcap/packages/ntopng-container-image/ntopng/ntopng.img

FILE=/usr/bin/wg
if test -f "$FILE"; then
    echo "$FILE exist"
else

    dpkg -i /var/vcap/packages/wireguard-deb/wireguard-tools.deb
    #dpkg -i /var/vcap/packages/wireguard-deb/wireguard-dkms.deb
    dpkg -i /var/vcap/packages/wireguard-deb/wireguard.deb
fi

